int main() {
  int *fp = fopen("testcase/temp.txt", "r");
  if (fp == 0)
    printf("ファイルがありません\n");
  
  if (fseek(fp, 0, 2) == -1)
    printf("だめ \n");
  int size = ftell(fp);
  if (fseek(fp, 0, 0) == -1)
    printf("dame\n");
  char *buf = calloc(1, size+2);
  fread(buf, size, 1, fp);
  fclose(fp);
  printf("%s\n", buf);
}
